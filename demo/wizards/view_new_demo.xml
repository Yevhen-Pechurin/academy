<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record model="ir.actions.act_window" id="action_new_demo">
        <field name="name">On Hand</field>
        <field name="res_model">demo.demo.create</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

    <record model="ir.ui.view" id="form_view_book_on_hand">
        <field name="name">On Hand</field>
        <field name="model">demo.demo.create</field>
        <field name="arch" type="xml">
              <form>
                    <sheet>
                          <group>
                                <field name="date"/>
                          </group>
                    </sheet>
                    <footer>
                          <button string="Create" name="action_create" type="object" class="oe_highlight"/>
                          <button string="Cancel" class="btn btn-secondary" special="cancel" />
                    </footer>
              </form>
        </field>
      </record>

</odoo>