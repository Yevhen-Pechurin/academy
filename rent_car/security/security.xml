<odoo>
    <data>
        <record id="module_category_rent_car" model="ir.module.category">
            <field name="name">Rental</field>
        </record>

        <record id="group_rental_user" model="res.groups">
            <field name="name">User</field>
            <field name="category_id" ref="module_category_rent_car"/>
            <field name="implied_ids" eval="[(4,ref('base.group_user'))]"></field>
        </record>

        <record id="group_rental_manager" model="res.groups">
            <field name="name">Manager</field>
            <field name="category_id" ref="module_category_rent_car"/>
            <field name="implied_ids" eval="[(4,ref('group_rental_user'))]"></field>
        </record>

        <record id="base.user_admin" model="res.users">
            <field name="groups_id" eval="[(4,ref('rent_car.group_rental_manager'))]"></field>
        </record>
    </data>
</odoo>