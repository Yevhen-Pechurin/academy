<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <record model="ir.actions.act_window" id="action_car_in_garage">
        <field name="name">In Garage</field>
        <field name="res_model">rental_car.car.in_garage</field>
        <field name="view_mode">form</field>
        <field name="target">new</field>
    </record>

    <record model="ir.ui.view" id="form_view_car_in_garage">
        <field name="name">In Garage</field>
        <field name="model">rental_car.car.in_garage</field>
        <field name="arch" type="xml">
              <form>
                    <sheet>
                            <group>
                                <field name="odometer_end_value"/>
                            </group>

                    </sheet>
                    <footer>
                            <button string="In Garage" type="object" name="action_back_to_garage" class="oe_highlight" />
                            <button string="Cancel" class="btn btn-secondary" special="cancel" />
                    </footer>
              </form>
        </field>
      </record>
</odoo>


