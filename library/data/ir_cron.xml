<?xml version="1.0" encoding="UTF-8" ?>
<odoo><data noupdate="1">
    <!-- Event Mail Scheduler-->
    <record model="ir.cron" forcecreate="True" id="_cron_overdue_book_notification">
        <field name="name">Library: sent overdue notification</field>
        <field name="model_id" ref="model_library_book"/>
        <field name="state">code</field>
        <field name="code">model._cron_overdue_book_notification()</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
        <field name="numbercall">-1</field>
        <field name="doall" eval="False" />
    </record>
</data></odoo>