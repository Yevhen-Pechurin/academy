    <record model="ir.cron" forcecreate="True" id="cron_overdue_book_notification">
        <field name="name">Libary: sent overdue notification</field>
        <field name="model_id" ref="model_library_book"/>
        <field name="state">code</field>
        <field name="code">model._cron_overdue_book_notification()</field>
        <field name="user_id" ref="base.user_root"/>
        <field name="interval_number">1</field>
        <field name="interval_type">days</field>
