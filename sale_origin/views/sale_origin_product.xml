<?xml version="1.0" encoding="UTF-8"?>
<odoo>
  <record id="view_country_of_origin_filed_product" model="ir.ui.view">
    <field name="model">product.template</field>
    <field name="inherit_id" ref="product.product_template_form_view"/>
    <field name="arch" type="xml">
      <data>
        <xpath expr="//field[@name='uom_id']" position="before">
           <field name="country_of_origin"  position="replace">
            <field name="country_of_origin" attrs="{'invisible': [('country_of_origin', '=', 'null')]}"/>
        </field>
          <field name="min_temperature_regime"/>
          <field name="max_temperature_regime"/>
          <field name="temperature_regime" options="{'no_edit': True, 'no_create': True,}"/>
        </xpath>
      </data>
    </field>
  </record>
</odoo>