<?xml version="1.0" encoding="UTF-8" ?>

<odoo>
    <record id="res_config_settings_view_form_ipstack" model="ir.ui.view">
        <field name="name">res.config.settings.view.form.ipstack</field>
        <field name="model">res.config.settings</field>
        <field name="inherit_id" ref="base.res_config_settings_view_form"/>
        <field name="arch" type="xml">
            <xpath expr="//div[@id='external_pads_setting']" position="before">
                <div class="col-12 col-lg-6 o_setting_box" id="ipstack_setting">
                    <div class="o_setting_pane">
                        <b>Ipstack Settings</b>
                        <div class="content_group">
                            <div class="row mt16">
                                <label for="ipstack_key" string="Access Key" class="col-lg-3 o_light_label"/>
                                <field name="ipstack_key" placeholder="ipstack_key"/>
                            </div>
                        </div>
                    </div>
                </div>
            </xpath>
        </field>
    </record>

</odoo>
