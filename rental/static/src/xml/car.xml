<?xml version="1.0" encoding="UTF-8"?>

<templates>

    <div t-name="rental.car">
        <button class="backward_btn">Move backward</button>
        <button class="forward_btn">Move forward</button>
        <span class="val">
            <t t-esc="widget.pos"/>
        </span>
    </div>


    <t t-name="rental.list_car">
        <div class="rental_search_div">
            <ul class="rental_search_ul list-unstyled">
                <li t-foreach="list_partner" t-as="car" class="rental_search_li">
                    <p class="list_person_span" t-esc="car['name']" t-att-data-id="car['id']"/>
                </li>
            </ul>
        </div>
    </t>

</templates>
