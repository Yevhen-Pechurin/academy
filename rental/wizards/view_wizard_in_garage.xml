<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

  <record model="ir.ui.view" id="form_view_wizard_in_garage">
    <field name="name">On Loan</field>
    <field name="model">rental.wizard.in_garage</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <group>
              <field name="finish_odometer"/>
            </group>
          </group>
        </sheet>
        <footer>
          <button string="In Garage" name="action_in_garage" type="object" class="oe_highlight" data-hotkey="q"/>
          <button string="Cancel" class="btn btn-secondary" special="cancel" data-hotkey="z"/>
        </footer>
      </form>
    </field>
  </record>

</odoo>