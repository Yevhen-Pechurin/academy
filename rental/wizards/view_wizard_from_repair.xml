<?xml version="1.0" encoding="UTF-8" ?>
<odoo>

  <record model="ir.ui.view" id="form_view_wizard_from_repair">
    <field name="name">From Repair</field>
    <field name="model">rental.wizard.from_repair</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <group>
              <field name="finish_odometer"/>
            </group>
          </group>
        </sheet>
        <footer>
          <button string="From Repair" name="action_from_repair" type="object" class="oe_highlight" data-hotkey="q"/>
          <button string="Cancel" class="btn btn-secondary" special="cancel" data-hotkey="z"/>
        </footer>
      </form>
    </field>
  </record>

</odoo>